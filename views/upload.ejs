<% include ./partials/header %>
			<div class="main">
				<div class="container">
	      	<div id="signupbox" style="margin-top:50px" class="mainbox col-md-8 col-md-offset-4 col-sm-2 col-sm-offset-2">
	          <div class="panel panel-info">
	            <div class="panel-heading">
	              <div class="panel-title"><b>Post your photos</b></div>

	            </div>
							<br>
	          <div class="panel-body" >
	            <form action="/profile/post" method="post" ref='uploadForm' id='uploadForm' encType="multipart/form-data" class="form-horizontal" role="form">
								<div class="form-group">
	                <label for="title" class="col-md-8 control-label">Title</label>
	                <div class="col-md-9">
	                  <input type="text" class="form-control" name="title" placeholder="Upload your photo">
	                </div>
	              </div>
	    					<div class="form-group">
	                <label for="imageUrl" class="col-md-8 control-label">Image URL</label>
	                <div class="col-md-9">
	                  <input type="file" class="form-control" name="imageUrl" placeholder="Image URL">
	                </div>
	              </div>
	              <div class="form-group">
	                <label for="Story" class="col-md-8 control-label">Story</label>
	                <div class="col-md-9">
	                  <input type="text" class="form-control" name="story" placeholder="Story">
	                </div>
	              </div>
	            </div>

								<div class="form-group">
	              <!-- Button -->
	                <div class="col-md-offset-3 col-md-9">
	                  <input id="btn-submit" type="submit" class="btn btn-success" value="Submit">
	                </div>
	              </div>
							</form>
	          </div>
	        	</div>
	       	</div>
  			</div>
			</div>
		</div><!-- container -->
<% include ./partials/footer %>
